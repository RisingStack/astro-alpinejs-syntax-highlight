{
	"name": "astro-alpinejs-syntax-highlight",
	"displayName": "Astro Alpine.js Syntax Highlight",
	"description": "Javascript syntax highlighting for Alpine.js x- attributes in Astro files",
	"author": "Tamas Kadlecsik",
	"license": "MIT",
	"version": "1.0.0",
	"icon": "assets/alpine_logo.png",
	"publisher": "RisingStack",
	"repository": {
		"type": "git",
		"url": "https://github.com/RisingStack/astro-alpinejs-syntax-highlight"
	},
	"keywords": [
		"alpine",
		"alpinejs",
		"alpine.js",
		"astro"
	],
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "inline-alpinejs"
			}
		],
		"grammars": [
			{
				"language": "inline-alpinejs",
				"path": "./syntaxes/injection.json",
				"scopeName": "astro.alpinejs.attribute",
				"injectTo": [
					"source.astro"
				],
				"embeddedLanguages": {
					"meta.embedded.inline.alpinejs": "javascript"
				}
			}
		]
	}
}